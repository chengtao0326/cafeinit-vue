    "webpack",
    "vue",
    "cafeinit"
  ],
  "author": "burning <iburning@live.cn>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/iburning/cafeinit-vue/issues"
  },
  "homepage": "https://github.com/iburning/cafeinit-vue#readme",
  "dependencies": {
    "vue": "^2.2.1",
    "vue-router": "^2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-latest": "^6.0.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "grunt": "^1.0.1",
